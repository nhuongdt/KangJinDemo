        }
        else {
            $(".tr-hide").hide();
        }
    });
    function printPage() {
        LoadingForm(true);
        var strPrint = '';
        var key_id = "#";
        var data_id;
        $('#table-reponsivetr .tabmenu').each(function (i) {
            if (!$(this).hasClass('active')) {
                $(this).hide();
            }
            else {
                data_id = $(this).data('id');
                key_id += $(this).attr('id');
            }
        });
        if (data_id === parseInt($('#ID_thhangnhapkho').val())
            || data_id === parseInt($('#ID_thhangxuatkho').val())) {
            $(".tab-show " + key_id + ' .tab-content .tab-pane').each(function () {
                if ($(this).hasClass('active')) {
                    var value = $(this).data('id');
                    $("#table-reponsivetr " + key_id + ' .tab-content .tab-pane').each(function () {
                        if ($(this).data('id') !== value) {
                            $(this).hide();
                        }
                    });
                }
            });
        }
        strPrint += document.getElementById('printtable').innerHTML;
        PrintExtraReportTr(strPrint);
        $('#table-reponsivetr .tabmenu').each(function () {
            if (!$(this).hasClass('active')) {
                $(this).show();
            }
        });
        if (data_id === parseInt($('#ID_thhangnhapkho').val())
            || data_id === parseInt($('#ID_thhangxuatkho').val())) {
            $(".tab-show " + key_id + ' .tab-content .tab-pane').each(function () {
                if ($(this).hasClass('active')) {
                    var value = $(this).data('id');
                    $("#table-reponsivetr " + key_id + ' .tab-content .tab-pane').each(function () {
                        if ($(this).data('id') !== value) {
                            $(this).show();
                        }
                    });
                }
            });
        }
