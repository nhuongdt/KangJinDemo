                    case '9':
                        self.BaoCaoTieuDe = "Báo cáo xuất hỗ trợ";
                        break;
                    case '10':
                        self.BaoCaoTieuDe = "Báo cáo doanh thu khách hàng";
                        break;
                    default:
                        break;
