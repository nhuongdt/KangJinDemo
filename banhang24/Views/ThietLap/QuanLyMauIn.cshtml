@using System.Web.Optimization
@using banhang24.Hellper;
@model  banhang24.Models.ModelMauIn
@{
    Layout = null;
    ViewBag.Title = "Open24.vn - Quản lý mẫu in";
}
<head>
    <style type="text/css">
        .note-print-page label {
        }

        .color-required {
            color: #7161ccdd
        }

        .cke_bottom {
            display: none;
        }

        .ifram-text {
            min-height: 600px;
            width: 100%;
            border: none;
        }

        #cke_txtCkeditor {
            width: 100% !important;
        }

        .note-print-page li p {
            color: #0097c4;
        }

        .border-bottom-dashed {
            border-bottom: 1px dashed black;
        }

        ul.note-print-page li:not(.title) {
            /* float: left; */
            line-height: 24px;
            padding: 3px;
            width: calc(100% - 30px);
            display: flex;
            justify-content: space-between;
            border-bottom: 1px solid #ccc;
            text-align: right;
            margin-left: 20px;
        }

        ul.note-print-page li label:hover {
            cursor: pointer;
            background: rgba(0,0,0,0.15);
            border-radius: 4px;
        }

        ul.note-print-page li label {
            width: unset;
            transition: all 0.4s ease;
        }

        ul.note-print-page li.title {
            font-weight: bold;
            font-size: 17px;
            height: 30px;
            background: #e6e6e6;
            padding: 5px;
            border: none;
            border-radius: 3px;
            padding: 3px 7px;
            line-height: 24px;
            height: 30px;
        }
    </style>
    <link href="~/Content/icomoon/style1.css" rel="stylesheet" />
    <script src="~/Scripts/tinymce/tinymce.min.js"></script>
</head>
