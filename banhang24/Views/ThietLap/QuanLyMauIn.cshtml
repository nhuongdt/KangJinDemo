    <div class="col-md-10 col-xs-12">
        <div style="display:flex; margin:15px 0">
            @if (Model.RolePermission.IsExits && Model.RolePermission.IsInsert)
            {
                <button type="button" class="btn btn-main height-30" style="width:unset" data-toggle="modal" data-bind="click:ShowPopupAddNew">
                    <i class="fa fa-plus" aria-hidden="true"></i>     <font>Thêm mới</font>
                </button>
            }
            <select class="form-control" style="max-width:250px;margin-right:5px;" id="selectMauIn" data-bind="options:ListMauIn, optionsText: 'Value', optionsValue: 'Key' ,value: @*newNhomDoiTuong().ID*@ selectedMauIn"></select>
            @if (Model.RolePermission.IsExits && Model.RolePermission.IsUpdate)
            {
                <button class="btn btn-main height-30 " data-bind="click:ShowPopupEdit,visible:CheckVisibleEdit" style="width:150px">
                    <i class="fa fa-edit"></i> Cập nhật mẫu in
                </button>
            }
        </div>
        <div class="print-customer establish-right " style="width:100% !important">
            <div class=" flex" id="dathang">
                <div class="col-md-6  bordermauin">
                    <textarea id="txtCkeditor">                </textarea>
                    <div class="flex" style="margin:15px; justify-content:flex-end">
                        @if (Model.RolePermission.IsExits && Model.RolePermission.IsCopy)
                        {
                            <button data-bind="click:CopyMauIn,visible: Showbutton" class="btn btn-main">
                                <i class="fa fa-cut"></i> Sao chép
                            </button>
                        }
                        @if (Model.RolePermission.IsExits && Model.RolePermission.IsUpdate)
                        {
                            <button class="btn btn-main " data-bind="click:EditDuLieuMauIn,visible: Showbutton">
                                <img src="/Content/images/icon/Iconthemmoi-14.png"> Lưu
                            </button>
                        }
                        @if (Model.RolePermission.IsExits && Model.RolePermission.IsDelete)
                        {
                            <button data-bind="click:DeleteMauIn,visible: Showbutton" class="btn btn-cancel">
                                <i class="fa fa-trash"></i> Xóa
                            </button>
                        }
                    </div>
                </div>
                <div class="col-md-6 out-print nopadding">
                    <iframe class="ifram-text" id="InputPrint" data-bind="iframeContent: dataIframe"></iframe>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade in" id="themmoi" tabindex="-1" aria-labelledby="myModalLabel">
        <div class="modal-dialog draggable " role="document">
            <div class="modal-content  ui-draggable ui-draggable-handle">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="material-icons">close</i></button>
                    <h4 class="modal-title" id="myModalLabel" data-bind="text:TitleEdit">
                        Thêm mới mẫu in
                    </h4>
                </div>
                <div class="modal-body floatleft">
                    <div class="form-group floatleft">
                        <label>Tên mẫu in</label>
                        <div class="form-news">
                            <input type="text" data-bind="value:TenMauInAddNew" class="form-control">
                        </div>
                    </div>
                    <div class="form-group floatleft">
                        <label>Khổ giấy</label>
                        <div class="form-news">
                            <select class="form-control" id="SelecttedKhoGiay">
                                <!--ko if: IsInvoice()-->
                                @if (!isGara)
                                {
                                    foreach (var item in commonEnum.Gara_MauInHoaDon
                                        .Where(o => !o.Key.Equals((int)commonEnum.TypeMauin.dangdung)))
                                    {
                                        <option value="@item.Key">@item.Value</option>
                                    }
                                }
                                else
                                {
                                    foreach (var item in commonEnum.DanhSachTypeMauIn.Where(o => !o.Key.Equals((int)commonEnum.TypeMauin.dangdung)))
                                    {
                                        <option value="@item.Key">@item.Value</option>
                                    }
                                }
                                <!--/ko-->
                                <!--ko if: !IsInvoice()-->
                                @foreach (var item in commonEnum.DanhSachTypeMauIn
                                                .Where(o => !o.Key.Equals((int)commonEnum.TypeMauin.dangdung)
                                                && !Array.Exists(arrayGara, e => e == o.Key)))
                                {
                                    <option value="@item.Key">@item.Value</option>
                                }
                                <!--/ko-->
                            </select>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <div class="container-fluid">
                        <button type="button" class="btn btn-cancel" data-dismiss="modal"><i class="fa fa-ban"></i> Bỏ qua</button>
                        <button type="button" data-bind="click:AddNewMauIn" class="btn btn-main"> <i class="fa fa-check-square"></i> Lưu</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
