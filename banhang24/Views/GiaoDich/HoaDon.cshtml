        }
        else {
            $(this).closest('th').attr('colspan', 1).removeClass('actioveFilter');
        }
        $(this).closest('th').find('.right-inner-addon').toggle();
    });
    var dathangTeamplate = '@commonEnum.MauInTeamPlates.HoaDon';
    var funcName = '@RoleKey.HoaDon';
</script>
