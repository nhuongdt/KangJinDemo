            if ($(this).css('display') !== 'none') {
                number += 1;
            }
        });
        if (number === 3) {
            $('#btnSave').removeClass('add-print');
            $('#btnSave').addClass('add-Save-draft');
        }
        else if (number === 2) {
            $('#btnSave').removeClass('add-Save-draft');
