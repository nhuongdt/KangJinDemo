                                            <span style="color:#ff6a00">{{ct.ThuocTinh_GiaTri}}</span>
                                            <span style="color: #8abb0f" v-if="ct.TenDonViTinh">
                                                (<span>{{ct.TenDonViTinh}}</span>)
