        if (!$('#vmThanhPhanDinhLuong').hasClass('in')) {
            if (vmHoaHongDV.saveOK) {
                if (!vmHoaHongDV.isNew) {
                    vmChiTietHoaDon.AssignNVTHucHien_toCTHD();
                }
            }
        }
    })
