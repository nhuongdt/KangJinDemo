                                        <th v-if="!$root.hideBottom">Thanh toán</th>
                                        <th style="text-align:center"
                                            v-if="formType!=2  && formType!==3 && !$root.isKhoaSo && $root.role.CapNhatHoaHongDichVu && $.inArray(HoaDon.LoaiHoaDon,[1,19,25,6]) > -1">#</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr v-for="(ct, index) in HoaDon.BH_HoaDon_ChiTiet">
                                        <td>
                                            <a href="javascript:void(0)"
                                               v-on:click="GotoOther(1,ct.MaHangHoa)">
                                                {{ct.MaHangHoa}}
                                            </a>
                                        </td>
                                        <td>
                                            <span>{{ct.TenHangHoaThayThe}}</span>
                                            <i class="fal fa-info-circle" title="Thành phần combo"
